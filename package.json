{
    "name": "@exabyte-io/made.js",
    "version": "2.1.3",
    "description": "Materials Design in Javascript",
    "scripts": {
        "prepublishOnly": "rm -rf lib; npm run transpile",
        "transpile": "mkdir -p lib; babel src --out-dir lib",
        "test": "mocha --recursive --bail --require @babel/register/lib --require tests/setup.js tests",
        "lint": "eslint src && prettier --write src",
        "lint:fix": "eslint --fix --cache src && prettier --write src",
        "prettier": "prettier",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Exabyte-io/made-js.git"
    },
    "main": "lib/made.js",
    "author": "Exabyte Inc.",
    "bugs": {
        "url": "https://github.com/Exabyte-io/made-js/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Exabyte-io/made-js",
    "engines": {
        "node": ">=0.12"
    },
    "devDependencies": {
        "@babel/cli": "7.1.0",
        "@babel/core": "7.1.0",
        "@babel/plugin-proposal-class-properties": "7.0.0",
        "@babel/polyfill": "7.0.0",
        "@babel/preset-env": "7.0.0",
        "@babel/register": "7.0.0",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.4",
        "chai-almost": "^1.0.1",
        "eslint": "^7.24.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^32.3.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "mocha": "^8.3.2",
        "prettier": "^2.2.1"
    },
    "dependencies": {
        "array-almost-equal": "^1.0.0",
        "crypto-js": "^3.1.9-1",
        "lodash": "^4.17.4",
        "mathjs": "^3.9.0",
        "mixwith": "^0.1.1",
        "underscore": "^1.8.3",
        "underscore.string": "^3.3.4",
        "@exabyte-io/periodic-table.js": "2.0.4"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    }
}
